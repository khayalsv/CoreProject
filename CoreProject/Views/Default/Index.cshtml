
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en-US">

@await Html.PartialAsync("HeaderPartial") 

<body id="top">
    @await Html.PartialAsync("NavbarPartial")
    <div class="page-content">
        <div id="content">
            @await Component.InvokeAsync("_Feature")
            @await Component.InvokeAsync("_About")
            @await Component.InvokeAsync("_Service")
            @await Component.InvokeAsync("_Skill")
            @await Component.InvokeAsync("_Portfolio")
            @await Component.InvokeAsync("_Experience")
            @await Component.InvokeAsync("_Testimonial")

            <div class="section px-2 px-lg-4 pb-4 pt-5 mb-5" id="contact">
                <div class="container-narrow">
                    <div class="text-center mb-5">
                        <h2 class="marker marker-center">Contact Me</h2>
                    </div>
                    <div class="row">
                        @await Html.PartialAsync("SendMessage")
                        @await Component.InvokeAsync("_ContactDetail")

                    </div>
                </div>
            </div>

            @await Component.InvokeAsync("_Footer")
        </div>
    </div>
    <div id="scrolltop"><a class="btn btn-secondary" href="#top"><span class="icon"><i class="fas fa-angle-up fa-x"></i></span></a></div>

    @await Html.PartialAsync("ScriptPartial")


</body>

</html>